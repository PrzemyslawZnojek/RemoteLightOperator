<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.remotelightoperator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".welcome.WelcomeActivity"
            android:label="Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".plantchooser.PlantListActivity"
            android:label="Plant List">
        </activity>
        <activity android:name=".plantchooser.PlantDescriptionActivity"
            android:label="Plant Description">
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".login.LoginActivity" android:label="Login">
        </activity>
    </application>

</manifest>